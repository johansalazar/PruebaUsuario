@model ProxyServices.Dto.Usuarios

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2 class="text-center">Detalle</h2>

<div>
    <h4 class="">Usuarios</h4>
    <hr />

    <table class="table table-striped table-sm">
        <tr>
            <th>Nombre</th>
            <td>@Model.Nombre</td>
        </tr>
        <tr>
            <th>Fecha Nacimiento</th>
            <td>@Model.FechaNacimiento</td>
        </tr>
        <tr>
            <th>Sexo</th>
            <td>@Model.Sexo</td>
        </tr>
    </table>
</div>
<p>
    @Html.ActionLink("Editar", "Edit", new { id = Model.Id_Usuario }, new { @class = "btn btn-sm btn-primary" }) |
    @Html.ActionLink("Back to List", "Index",null, new { @class = "btn btn-sm btn-success" })
</p>
