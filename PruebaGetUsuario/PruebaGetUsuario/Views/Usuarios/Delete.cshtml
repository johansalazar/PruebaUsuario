@model ProxyServices.Dto.Usuarios

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2 class="text-center">Eliminar</h2>

<h3>¿Esta seguro de borrar este registro?</h3>
<div>
    <h4 class="text-center">Usuarios</h4>
    <hr />
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()
        <table class="table table-striped table-sm">         
            <tr>
                <th>Nombre</th>
                <td>@Model.Nombre</td>
            </tr>
            <tr>
                <th>Fecha Nacimiento</th>
                <td>@Model.FechaNacimiento</td>
            </tr>
            <tr>
                <th>Sexo</th>
                <td>@Model.Sexo</td>
            </tr>
        </table>
        <input type="text" name="Id_Usuario" id="Id_Usuario" value="@Model.Id_Usuario" hidden/>



        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-sm btn-primary" /> |
            @Html.ActionLink("Regresar", "Index", null, new { @class = "btn btn-sm btn-success" })
        </div>
    }
</div>
